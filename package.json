{
  "name": "@softwaregardeners/aha",
  "description": "automatic wordpress provisionning using node, terraform and kubernetes",
  "version": "1.0.0",
  "author": "Brice Carpentier @bricecarpentier",
  "bin": {
    "aha": "./bin/run"
  },
  "bugs": "https://github.com/bricecarpentier/aha/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/fs-extra": "^8.1.0",
    "@types/listr": "^0.14.2",
    "@types/mustache": "^4.0.1",
    "fs-extra": "^9.0.0",
    "listr": "^0.14.3",
    "mustache": "^4.0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/templates/**/*.tf",
    "/templates/**/*.yaml",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bricecarpentier/aha",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "aha",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bricecarpentier/aha",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
